  "description": "EM Tool",
  "angular-cli": {},
  "scripts": {
    "ng": "ng",
    "build": "ng build",
    "start": "node dist/server/app.js",
    "predev": "tsc -p server",
    "dev": "concurrently \"mongod --config /usr/local/etc/mongod.conf\" \"ng serve --proxy-config proxy.conf.json --open\" \"tsc -w -p server\" \"nodemon dist/server/app.js\"",
    "serve": "concurrently \"mongod --config /usr/local/etc/mongod.conf\" \"ng serve --aot --prod --proxy-config proxy.conf.json --open\" \"tsc -p server\" \"node dist/server/app.js\"",
    "prod": "concurrently \"mongod --config /usr/local/etc/mongod.conf\" \"ng build --aot --prod && tsc -p server && node dist/server/app.js\"",
    "test": "ng test",
    "testbe": "tsc -p server && node_modules/.bin/mocha dist/server/test",
    "lint": "ng lint",
    "lintbe": "tslint server/**/**.ts{,x}",
    "e2e": "ng e2e"
  },
  "private": true,
